<table class="table table-striped">
  <caption>Users table</caption>
  <thead>
    <tr>
      <th>#id</th>
      <th>#role</th>
      <th>#email</th>
      <th>#actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.is %></td>
      <td><%= user.email %></td>
      <td></td>
    </tr>
    <% end %>
  </tbody>
</table>